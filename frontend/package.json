{
  "name": "bicpv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tailwind:init": "tailwindcss init -p",
    "test": "vitest run --exclude=**/*.e2e.test.tsx",
    "test:watch": "vitest --exclude=**/*.e2e.test.tsx",
    "test:ui": "vitest --ui --exclude=**/*.e2e.test.tsx",
    "test:coverage": "vitest run --coverage --exclude=**/*.e2e.test.tsx",
    "test:e2e": "vitest run src/__tests__/**/*.e2e.test.tsx",
    "test:e2e:watch": "vitest src/__tests__/**/*.e2e.test.tsx",
    "test:all": "vitest run"
  },
  "dependencies": {
    "@swc/core": "^1.13.5",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^7.1.2",
    "vitest": "^2.1.8"
  }
}
